{
  "compilerOptions": {
    "baseUrl": ".",
    "lib": [
      "ES2022"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "./out",
    "target": "ES2022",
    "esModuleInterop": true,
    "inlineSourceMap": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,
    "preserveConstEnums": true,
    "removeComments": false,
    "sourceMap": true,
    "strict": true
  },
  "files": [
    "src/extension.ts"
  ]
}