{
    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "bundler",
        "target": "esnext",
        "lib": [
            "esnext"
        ],
        "baseUrl": ".",
        "rootDir": "src",
        "esModuleInterop": true,
        "isolatedModules": true,
        "noEmit": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
